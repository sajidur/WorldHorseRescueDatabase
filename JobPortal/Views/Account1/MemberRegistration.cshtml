
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0,minimal-ui">
    <title>Registration</title>
    <meta content="Admin Dashboard" name="description">
    <meta content="Themesbrand" name="author">
    <link rel="shortcut icon" href="rex/assets/images/favicon.ico">
    <link href="~/rex/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="~/rex/assets/css/metismenu.min.css" rel="stylesheet" type="text/css">
    <link href="~/rex/assets/css/icons.css" rel="stylesheet" type="text/css">
    <link href="~/rex/assets/css/style1.css" rel="stylesheet" type="text/css">
    <style type="text/css">
        .wrapper-page {
            margin: 2.5% auto;
            max-width: 700px;
            position: relative;
        }

        label {
            font-weight: bold;
        }

        .text-style {
            margin-top: -90px;
        }

        .card-body h3 {
            margin-left: 15px !important;
        }



        .form-control {
            font-size: 14px;
        }

        .text-muted {
            color: #000000 !important;
        }
    </style>
</head>

<body>
    <!-- Begin page -->
    <div class="wrapper-page">
        <div class="card">
            <div class="card-body">
                <h3 class="text-left m-0"><a href="" class="logo logo-admin"><img src="~/rex/assets/images/membershipicon.png" height="100" alt="logo"></a></h3>
                <div class="p-3">
                    <div class="text-style">
                        <h4 class="text-muted  m-b-5 text-center page-title"><strong>ARHA Membership Application</strong></h4>
                        <p class="text-muted text-center"></p>
                    </div>
                    <br>
                    <form class="form-horizontal m-t-30" action="">
                        <div class="form-group">
                            <label for="Name"> <strong>Member Name</strong></label>
                            <input type="text" class="form-control" id="Name" placeholder="Name" name="Name">
                        </div>
                        <div class="form-group">
                            <label for="Number">Member Number</label>
                            <input type="text" class="form-control" id="Number" placeholder="Member Number" name="MemberNumber">
                        </div>
                        <div class="form-group">
                            <label for="Address">Member Address</label>
                            <input type="text" class="form-control" id="Address" placeholder="Address" name="Address">
                        </div>

                        <div class="form-group">
                            <label for="Phone">Member Phone</label>
                            <input type="text" class="form-control" id="Phone" placeholder="Phone" name="Phone">
                        </div>
                        <div class="form-group">
                            <label >Member Email</label>
                            <input type="email" class="form-control"  placeholder="Email" name="Email">
                        </div>

                        <div class="form-group">
                            <label for="MembershipType">Membership Type</label>
                            <select class="form-control" id="MembershipType"name="MembershipType">
                                <option>Select</option>
                                <option value="Youth">Youth</option>
                                <option value="Adult">Adult</option>
                            </select>

                        </div>


                        <div class="form-group">
                            <label for="userpassword">Membership Status</label>
                            <select class="form-control" name="MembershipStatus">
                                <option>Select</option>
                                <option value="Open">Open</option>
                                <option value="Amature">Amature</option>
                                <option value="Youth">Youth</option>
                            </select>

                        </div>

                        <div class="form-group row m-t-20">
                            <div class="col-12 text-right">
                                <button class="btn btn-primary w-md waves-effect waves-light" type="submit">Submit</button>
                            </div>
                        </div>
                        <div class="form-group m-t-10 mb-0 row">

                        </div>
                    </form>
                </div>
            </div>
        </div>
        <div class="m-t-40 text-center">
            <!--   <p>Already have an account ? <a href="pages-login.html" class="text-primary">Login</a></p> -->
            <p>© 2019 <a href="https://www.rexsystemsbd.com">Rex System Bangladesh</a></p>
        </div>
    </div>
    <!-- jQuery  -->
    <script src="~/rex/assets/js/jquery.min.js"></script>
    <script src="~/rex/assets/js/bootstrap.bundle.min.js"></script>
    <script src="~/rex/assets/js/metisMenu.min.js"></script>
    <script src="~/rex/assets/js/jquery.slimscroll.js"></script>
    <script src="~/rex/assets/js/waves.min.js"></script>
    <script src="~//plugins/jquery-sparkline/jquery.sparkline.min.js"></script>
    <!-- App js -->
    <script src="rex/assets/js/app.js"></script>
</body>
</html>
